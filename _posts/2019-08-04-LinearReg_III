    t0 <span class="o">&lt;-</span> <span class="m">1000</span> <span class="o">+</span> <span class="m">1</span><span class="p">;</span> <span class="bp">T</span> <span class="o">&lt;-</span> <span class="kp">dim</span><span class="p">(</span>d<span class="o">$</span>b<span class="p">)[</span><span class="m">1</span><span class="p">];</span> <span class="c1">## define burning period.</span>
    
